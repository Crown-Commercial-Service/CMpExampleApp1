<h1>{{title}}</h1>
<p>Welcome to the {{title}}</p>
{{#if errorMessage}}
    <p>{{errorMessage}}</p>
{{/if}}
<br>
<h3>Environment Variables.</h3>
<table border="2">
    <tr><th>Property</th><th>Value</th></tr>
    {{#each localEnv }}
        <tr><td>{{@key}}</td><td>{{this}}</td></tr>
    {{/each}}
</table>
<br>
